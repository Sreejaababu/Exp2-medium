#include <stdio.h>
int main()
{
    char ch;
    printf("enter a character");
    scanf("%c",&ch);
    if((ch>='a'&&ch<='z')||(ch>='A'&&ch<='Z'))
    {
        printf("%c is a alphabets ",ch);
    }
    else if (ch>='0'&&ch<='a')
    {
        printf("%c is a digit",ch);
    }
    else
    {
        printf("%c is a special character",ch);
    }
    return 0;
}
